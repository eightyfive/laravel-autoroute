{
    "name": "eyf/laravel-autoroute",
    "description": "Laravel routes as array (think YAML)",
    "license": "MIT",
    "authors": [
        {
            "name": "Benoit Sagols",
            "email": "benoit.sagols@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "hassankhan/config": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Eyf\\Autoroute\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Eyf\\Autoroute\\AutorouteServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^7.9",
        "phpunit/phpunit": "^9",
        "symfony/yaml": "^5.0"
    }
}
