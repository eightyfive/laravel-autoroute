{
  "name": "eyf/laravel-autoroute",
  "description": "Laravel YAML routes",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "yaml",
    "routing"
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter",
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "authors": [
    {
      "name": "0x55",
      "email": "0xfiftyfive@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "cebe/php-openapi": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Eyf\\Autoroute\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Eyf\\Autoroute\\AutorouteServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^7.9",
    "phpunit/phpunit": "^9.5",
    "laravel/framework": "^9.34",
    "laravel/sanctum": "^3.0"
  }
}
